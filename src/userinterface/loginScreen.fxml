      <Button fx:id="btnLogin" layoutX="136.0" layoutY="174.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <MenuBar layoutY="2.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TextField fx:id="txtFieldError" disable="true" editable="false" layoutY="213.0" prefHeight="25.0" prefWidth="318.0" style="-fx-text-fill: black;" />
   </children>
</AnchorPane>
